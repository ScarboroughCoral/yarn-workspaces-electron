{
  "name": "frontend",
  "version": "1.0.0",
  "homepage": "./",
  "scripts": {
    "watch": "react-app-rewired start",
    "build": "rimraf ../backend/build-frontend; mkdir ../backend/build-frontend; react-app-rewired build; cpx \"build/**\" \"../backend/build-frontend\"",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@app/shared": "1.0.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/semver": "^7.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "typescript": "4.1.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "react-app-rewire-yarn-workspaces": "^1.0.3",
    "react-app-rewired": "^2.1.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
