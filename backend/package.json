{
  "name": "backend",
  "version": "1.0.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.4"
  },
  "scripts": {
    "clean": "rimraf ../electron/build-backend; mkdir ../electron/build-backend",
    "copy-html": "cp ./src/server-dev.html ../electron/build-backend/server-dev.html",
    "watch": "yarn clean; yarn copy-html; tsc --build --force -w --preserveWatchOutput",
    "build": "yarn clean; yarn copy-html; tsc --build --force"
  },
  "dependencies": {
    "@app/shared": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
}
